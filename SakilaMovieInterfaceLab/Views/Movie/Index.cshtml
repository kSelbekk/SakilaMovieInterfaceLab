@model SakilaMovieInterfaceLab.ViewModels.MovieIndexViewModel

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Title</th>
            <th scope="col">Relese Year</th>
        </tr>
    </thead>
    @foreach (var i in Model.Films)
    {
        <tbody>
            <tr>
                <td>
                    @i.Title
                </td>
                <td>
                    @i.RelesYear
                </td>
            </tr>
        </tbody>

    }
</table>